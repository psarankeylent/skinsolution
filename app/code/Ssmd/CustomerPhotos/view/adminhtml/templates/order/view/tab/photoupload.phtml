    function uploadPhotofunction(photoId){

        var validFiles = ["jpg", "jpeg", "png","heic"];
        var str = document.getElementById(photoId).value;
        //alert(str);
        var res = '';
        if(str != "")
        {
            res = str.split(".");
            res = res[1];
        }
        var validImg = validFiles.includes(res);
        if(validImg == false)
        {
            alert('Please Upload Valid Image!');
            return false;
        }

        var form = jQuery('#form_photo_face')[0];
        var formData = new FormData(form);

        formData.append('form_key', window.FORM_KEY);


        url = '<?php echo $url ?>';

        jQuery.ajax({
            url: url,
            type: "POST",
            dataType: "JSON",
            contentType: false,
            enctype: 'multipart/form-data',
            processData: false, /* Important */
            data: formData,
            showLoader: true,
            cache: false,
            success: function(response){
                //alert('success');
                //jQuery("#face_photo").val(null);

                if(response.face_image_path != "")
                {
                    //alert(1);
                    //alert(response.face_image_path);
                    jQuery('.head-forward').html(response.face_image_path);
                }

            },
            error: function(response)
            {
                alert('Something went wrong!');
            }
        });
        return;
    }

