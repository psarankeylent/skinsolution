                {
                    alert('Some of photos missing to upload!');
                    //alert('call');
                    $('#order_syncphotos').attr('onclick',"");
                    $('.modal-popup').remove();
                    $('.modals-overlay').remove();
                    $('body').removeClass("_has-modal");
                    event.preventDefault();
                    return false;
                }

            });

            $("#face_photo").change(function(event){
                var target = event.target || event.srcElement;
