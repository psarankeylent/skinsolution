<?xml version="1.0"?>
<schema xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Setup/Declaration/Schema/etc/schema.xsd">
    <table name="vh_orders_log" resource="default" engine="innodb" >
        <column xsi:type="int" name="id" padding="11" unsigned="true" nullable="false" identity="true" comment="Id"/>
        <column xsi:type="varchar" name="response_status" nullable="true" length="55" />
        <column xsi:type="varchar" name="response_msg" nullable="true" length="255" />
        <column xsi:type="varchar" name="magento_order_id" nullable="true" length="55" />
        <column xsi:type="varchar" name="order_grand_total" nullable="true" length="55" />
        <column xsi:type="text" name="order_items_arr" nullable="true" />
        <column xsi:type="varchar" name="order_type" nullable="true" length="55" />
        <column xsi:type="varchar" name="order_address_1" nullable="true" length="255" />
        <column xsi:type="varchar" name="order_address_2" nullable="true" length="255" />
        <column xsi:type="varchar" name="order_city" nullable="true" length="55" />
        <column xsi:type="varchar" name="order_address_type" nullable="true" length="55" />
        <column xsi:type="varchar" name="order_state" nullable="true" length="55" />
        <column xsi:type="varchar" name="order_state_id" nullable="true" length="55" />
        <column xsi:type="varchar" name="order_patient_phone" nullable="true" length="55" />
        <column xsi:type="varchar" name="order_patient_fname" nullable="true" length="55" />
        <column xsi:type="varchar" name="order_patient_lname" nullable="true" length="55" />
        <column xsi:type="varchar" name="order_patient_id" nullable="true" length="55" />
        <column xsi:type="varchar" name="order_patient_email" nullable="true" length="99" />
        <column xsi:type="varchar" name="order_patient_gender" nullable="true" length="55" />
        <column xsi:type="text" name="response_json" nullable="true" />
        <column xsi:type="timestamp" name="created_at" on_update="false" nullable="false" default="CURRENT_TIMESTAMP"
                comment="Created At"/>
        <column xsi:type="timestamp" name="updated_at" on_update="true" nullable="false" default="CURRENT_TIMESTAMP"
                comment="Updated At"/>
        <constraint xsi:type="primary" referenceId="PRIMARY">
            <column name="id"/>
        </constraint>
    </table>
</schema>


