<?xml version="1.0" ?>
<listing xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Ui:etc/ui_configuration.xsd">
	<argument name="data" xsi:type="array">
		<item name="js_config" xsi:type="array">
			<item name="provider" xsi:type="string">prescriptions_expiring_prescription_expire_schedule_listing.prescriptions_expiring_prescription_expire_schedule_listing_data_source</item>
		</item>
	</argument>
	<settings>
		<spinner>prescriptions_expiring_prescription_expire_schedule_columns</spinner>
		<deps>
			<dep>prescriptions_expiring_prescription_expire_schedule_listing.prescriptions_expiring_prescription_expire_schedule_listing_data_source</dep>
		</deps>

	</settings>
	<dataSource name="prescriptions_expiring_prescription_expire_schedule_listing_data_source" component="Magento_Ui/js/grid/provider">
		<settings>
			<storageConfig>
				<param name="indexField" xsi:type="string">prescription_expire_schedule_id</param>
			</storageConfig>
			<updateUrl path="mui/index/render"/>
		</settings>
		<aclResource>Prescriptions_Expiring::prescription_expire_schedule</aclResource>
		<dataProvider name="prescriptions_expiring_prescription_expire_schedule_listing_data_source" class="Magento\Framework\View\Element\UiComponent\DataProvider\DataProvider">
			<settings>
				<requestFieldName>id</requestFieldName>
				<primaryFieldName>prescription_expire_schedule_id</primaryFieldName>
			</settings>
		</dataProvider>
	</dataSource>
	<listingToolbar name="listing_top">
		<settings>
			<sticky>true</sticky>
		</settings>
		<bookmark name="bookmarks"/>
		<columnsControls name="columns_controls"/>
		<filters name="listing_filters"/>
		<paging name="listing_paging"/>
	</listingToolbar>
	<columns name="prescriptions_expiring_prescription_expire_schedule_columns">
		<settings>
			<editorConfig>
				<param name="selectProvider" xsi:type="string">prescriptions_expiring_prescription_expire_schedule_listing.prescriptions_expiring_prescription_expire_schedule_listing.prescriptions_expiring_prescription_expire_schedule_columns.ids</param>
				<param name="enabled" xsi:type="boolean">true</param>
				<param name="indexField" xsi:type="string">prescription_expire_schedule_id</param>
				<param name="clientConfig" xsi:type="array">
					<item name="saveUrl" xsi:type="url" path="prescriptions_expiring/prescriptionexpireschedule/inlineEdit"/>
					<item name="validateBeforeSave" xsi:type="boolean">false</item>
				</param>
			</editorConfig>
			<childDefaults>
				<param name="fieldAction" xsi:type="array">
					<item name="provider" xsi:type="string">prescriptions_expiring_prescription_expire_schedule_listing.prescriptions_expiring_prescription_expire_schedule_listing.prescriptions_expiring_prescription_expire_schedule_columns_editor</item>
					<item name="target" xsi:type="string">startEdit</item>
					<item name="params" xsi:type="array">
						<item name="0" xsi:type="string">${ $.$data.rowIndex }</item>
						<item name="1" xsi:type="boolean">true</item>
					</item>
				</param>
			</childDefaults>
		</settings>

		<column name="id">
			<settings>
				<filter>text</filter>
				<sorting>asc</sorting>
				<label translate="true">ID</label>
			</settings>
		</column>
		<column name="intervel_days">
			<settings>
				<filter>text</filter>
				<label translate="true">Run every (days)</label>
				<editor>
					<editorType>text</editorType>
					<validation>
						<rule name="required-entry" xsi:type="boolean">false</rule>
					</validation>
				</editor>
			</settings>
		</column>
		<column name="Experation_days">
			<settings>
				<filter>text</filter>
				<label translate="true">Prescriptions expiring in (days)</label>
				<editor>
					<editorType>text</editorType>
					<validation>
						<rule name="required-entry" xsi:type="boolean">false</rule>
					</validation>
				</editor>
			</settings>
		</column>
		<column name="next_run">
			<settings>
				<filter>text</filter>
				<label translate="true">Next run</label>
				<editor>
					<editorType>text</editorType>
					<validation>
						<rule name="required-entry" xsi:type="boolean">false</rule>
					</validation>
				</editor>
			</settings>
		</column>
		<column name="updated_at">
			<settings>
				<filter>text</filter>
				<label translate="true">Updated At</label>
				<editor>
					<editorType>text</editorType>
					<validation>
						<rule name="required-entry" xsi:type="boolean">false</rule>
					</validation>
				</editor>
			</settings>
		</column>
		<!--
		<actionsColumn name="actions" class="Prescriptions\Expiring\Ui\Component\Listing\Column\PrescriptionexpirescheduleActions">
            <argument name="data" xsi:type="array">
                <item name="config" xsi:type="array">
                    <item name="resizeEnabled" xsi:type="boolean">false</item>
                    <item name="resizeDefaultWidth" xsi:type="string">107</item>
                    <item name="indexField" xsi:type="string">id</item>
                </item>
            </argument>
        </actionsColumn>
    	-->
	</columns>
</listing>

